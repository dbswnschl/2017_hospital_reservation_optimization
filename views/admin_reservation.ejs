                                      <%
                                      for(var i = 0 ; i < list.length ; i ++){
                                          
                                      
                                      %>

		                            <tr>
                                        <td> <%= i+1%></td>
                                        <td> <%= acc[list[i].usernumber-1].userid%></td>
                                        <td> <%= acc[list[i].usernumber-1].username%></td>
                                        <td> <%= acc[list[i].usernumber-1].birth%></td>
                                        <td> <%= list[i].reservationtime%></td>
                                        <td> <%= list[i].part%></td>
                                        <td> 
                                        <% if(list[i].status ==0) {%>
                                        예약중
                                        <% }else if(list[i].status !=0) {%>
                                        예약만료<%}%>
                                        </td>
                                        <td><a href='#'>수정하기</a></td>
                                    </tr>
                                  <%} %>
		                          
