    <script>
var Script = function () {
    
        var barChartData2 = {
        labels : ["월","화","수","목","금"],
        datasets : [
            {
                fillColor : "rgba(220,220,220,0.5)",
                strokeColor : "rgba(220,220,220,1)",
                data : [<%= last_people[1]%>,<%= last_people[2]%>,<%= last_people[3]%>,<%= last_people[4]%>,<%= last_people[5]%>]
            },
            {
                fillColor : "rgba(151,187,205,0.5)",
                strokeColor : "rgba(151,187,205,1)",
                data : [0,0,0,0,0]
            }
        ]

    };
        var lineChartData2 = {
        labels : ["9시","10시","11시","13시","14시","15시","16시"],
        datasets : [
            {
                fillColor : "rgba(220,220,220,0.5)",
                strokeColor : "rgba(220,220,220,1)",
                pointColor : "rgba(220,220,220,1)",
                pointStrokeColor : "#fff",
                data : [7,9,7,4,8,5,16]
            },
            {
                fillColor : "rgba(151,187,205,0.5)",
                strokeColor : "rgba(151,187,205,1)",
                pointColor : "rgba(151,187,205,1)",
                pointStrokeColor : "#fff",
                data : [5,8,8,3,13,4,18]
            }
        ]

    };
    new Chart(document.getElementById("bar2").getContext("2d")).Bar(barChartData2);
    new Chart(document.getElementById("line2").getContext("2d")).Line(lineChartData2);

}();
</script>
