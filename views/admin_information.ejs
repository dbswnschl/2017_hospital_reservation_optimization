<!-- Modal -->
<%
for(var i = 0 ; i < acc.length ; i ++){
 %>
		          <div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1" id="edit_<%= i%>" class="modal fade">
		              <div class="modal-dialog">
		                  <div class="modal-content">
		                      <div class="modal-header">
		                          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
		                          <h4 class="modal-title">환자 정보를 수정합니다</h4>
		                      </div>
                              <form action="/pc/information_edit" method="post">
		                      <div class="modal-body">
                                  <input  type="hidden" name="id" value="<%= acc[i].id%>" autocomplete="off"  ><br/>
		                          <p>환자 아이디</p>
		                          <input type="text" name="userid" value="<%= acc[i].userid%>" autocomplete="off" class="form-control placeholder-no-fix"><br/>
                                  <p>환자 비밀번호</p>
                                  <input type="password" name="userpw" value="<%= acc[i].userpw%>" autocomplete="off" class="form-control placeholder-no-fix">
		                          <p>환자 성명</p>
                                  <input type="text" name="username" value="<%= acc[i].username%>" autocomplete="off" class="form-control placeholder-no-fix">
		                          <p>환자 생일</p>
                                  <input type="birth" name="birth" value="<%= acc[i].birth%>" autocomplete="off" class="form-control placeholder-no-fix">
		                          <p>환자 이메일</p>
                                  <input type="email" name="email" value="<%= acc[i].email%>" autocomplete="off" class="form-control placeholder-no-fix">
		                          <p>환자 전화번호</p>
                                  <input type="phone" name="phone" value="<%= acc[i].phone%>" autocomplete="off" class="form-control placeholder-no-fix">
		                          <p>계정 권한</p>
                                  <input type="text" name="auth" value="<%= acc[i].auth%>" autocomplete="off" class="form-control placeholder-no-fix">
		                      
                              
                              </div>
		                      <div class="modal-footer">
		                          <button data-dismiss="modal" class="btn btn-default" type="button">Cancel</button>
		                          <button class="btn btn-theme" type="submit">Confirm</button>
		                      </div>
                              </form>
		                  </div>
		              </div>
		          </div>

<%
}
 %>                  
